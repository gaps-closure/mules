profile PROF1 { hardware CLOSURE; }

pipeline P1 {
  entry => B1;
  B1    => exit;
}

block B1 {
  redact_altitude;
}

table sample_acl {
  || mux | sec | typ | op       ||
--||-----+-----+-----+----------||
  || 1   | 1   |  1  | 'deny'   ||
  || 1   | 1   |  2  | 'allow'  ||
  || 2   | 2   |  1  | 'redact' ||  
}

rule redact_altitude {
  if 
    whatevr 
  then 
    nop;
}

