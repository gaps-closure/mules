							],
							"pattern": "^.*$"
						},
						"wayPoints": {
							"$id": "#root/missionPlan/vehiclePlan/wayPoints", 
							"title": "Waypoints", 
							"type": "array",
							"numElements": 5,
							"default": [],
							"items":{
								"$id": "#root/missionPlan/vehiclePlan/wayPoints/items", 
								"title": "Items", 
								"type": "object",
								"required": [
									"wayPointId",
									"x",
									"y",
									"z"
								],
								"properties": {
									"wayPointId": {
										"$id": "#root/missionPlan/vehiclePlan/wayPoints/items/wayPointId", 
										"title": "Waypointid", 
										"type": "string",
                                                                                "maxLength": 32,
										"default": "",
										"examples": [
											"wp0"
										],
										"pattern": "^.*$"
									},
									"x": {
										"$id": "#root/missionPlan/vehiclePlan/wayPoints/items/x", 
										"title": "X", 
										"type": "number",
										"examples": [
											0.1
										],
										"default": 0.0
									},
									"y": {
										"$id": "#root/missionPlan/vehiclePlan/wayPoints/items/y", 
										"title": "Y", 
										"type": "number",
										"examples": [
											0.1
										],
										"default": 0.0
									},
									"z": {
										"$id": "#root/missionPlan/vehiclePlan/wayPoints/items/z", 
										"title": "Z", 
										"type": "number",
										"examples": [
											0.1
										],
										"default": 0.0
									}
								}
							}

						}
					}
				}

